{% extends 'base.html.twig' %}

{% block title %}Ticket{% endblock %}

{% block body %}
    <h1>Ticket</h1>

    <table class="table table-striped table-dark">
        <tbody>
            <tr>
                <th>Annee</th>
                <td>{{ ticket.annee }}</td>
            </tr>
            <tr>
                <th>NumeroTicket</th>
                <td>{{ ticket.numeroTicket }}</td>
            </tr>
            <tr>
                <th>DateVente</th>
                <td>{{ ticket.dateVente ? ticket.dateVente|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
           
        </tbody>
    </table>

    <a href="{{ path('app_tickets_index') }}">back to list</a>
    
    {{ include('tickets/_delete_form.html.twig') }}
{% endblock %}