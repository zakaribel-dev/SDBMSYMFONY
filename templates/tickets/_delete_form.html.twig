<form method="post" action="{{ path('app_tickets_delete', {'annee': ticket.annee}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ ticket.annee) }}">
  <br>
    <button class="btn btn-danger">Supprimer ce ticket</button>
</form>
