<form method="post" action="{{ path('app_vendres_delete', {'idArticle': vendre.idArticle, 'numeroTicket' : numeroTicket}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ vendre.idArticle) }}">
    <button class="btn">Delete</button>
</form>
